  "betterScripts": {
    "server": {
      "command": "node server.js"
    },
    "build": {
      "command": "webpack --progress --config ./webpack/config.prd.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:stg": {
      "command": "webpack --progress --config ./webpack/config.stg.js",
      "env": {
        "NODE_ENV": "staging"
      }
    },
    "dev": {
      "command": "webpack-dev-server --inline --hot --config ./webpack/config.dev.js",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  },
